<?xml version="1.0"?>
<!--
Servlet /GetPage
chiede una serie di pagine per sezione
-->
<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="getpages">
    <xs:complexType>
      <xs:sequence>
        <xs:element type="xs:string" name="section"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
